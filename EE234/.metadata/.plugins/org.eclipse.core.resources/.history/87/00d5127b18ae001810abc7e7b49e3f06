.text
.global main

.set LED_ADDR,0x4BB00000

main:
LDR r0,=LED_ADDR
bl init_PWM
bl init_PWM_PERIOD

init_PWM:
MOV r1,#0x0
STR r1, [r0] //Reset LEDs
MOV r1, #0x11
STR r1, [r0] //enable PWM and led 0
bx lr

init_PWM_PERIOD:
MOV r1, #0xC
