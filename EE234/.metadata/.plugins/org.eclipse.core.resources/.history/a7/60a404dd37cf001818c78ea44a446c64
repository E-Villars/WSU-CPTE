#ifndef SRC_MYFUNCTIONS_H_
#define SRC_MYFUNCTIONS_H_
#include <stdint.h>
#include <stdio.h>
#include "platform.h"
#include "xil_printf.h"


#endif /* SRC_MYFUNCTIONS_H_ */

void activateLEDS();
void disableInterrupts();
void GICConfigure();
void initalizeGPIOInterrupts();
#define uint32_t * interruptProcessorTargets= (uint32_t*)0xF8F01800
#define uint32_t * interruptPriorityMask =  (uint32_t*)0xF8F00104
#define uint32_t * cpuInterfaceControl = (uint32_t*)0xF8F00100
#define uint32_t * distributorControlReg = (uint32_t*)0xF8F01000
#define uint32_t * interruptSetEnable =  (uint32_t*)0xF8F01100
#define uint32_t * interruptClearEnable =  (uint32_t*) 0xF8F01180
#define uint32_t * interruptPriorityReg = (uint32_t*) 0xF8F01400
#define uint32_t * interruptConfigReg = (uint32_t*) 0xF8F01C00
